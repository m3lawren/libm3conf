Action(2, rb) = r5
Action(2, sec) = r5
Action(2, $) = r5
Action(2, id) = s4
Action(3, rb) = r1
Action(3, $) = r1
Action(3, sec) = s5
Action(4, eq) = s6
Action(5, id) = s7
Action(6, VALUE) = s8
Action(6, str) = s9
Action(6, int) = s10
Action(7, lb) = s11
Action(8, semi) = s12
Action(9, semi) = r6
Action(10, semi) = r7
